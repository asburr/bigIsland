{
    "e66f0aec-e9e9-4df4-987e-136c63c23f5c": {
        "6026697b-bc99-493f-b970-42107f95a3f7": {
            "version": "1",
            "cmd": "files",
            "params":  {
                "root": "samples.files",
                "path": {
                    "path": "test/Hallelulajah/pcap",
                    "depth": 1,
                    "readonly": "test/Hallelulajah/pcap_processed",
                    "feeds": [
                        {"feed": "{root}.pcap", "regex": ".*\\.pcap"},
                        {"feed": "{root}.new", "regex": ".*"}
                    ]
                }
            }
        }, "720ff709-f7eb-47db-bff1-a22ee4202582": {
            "version": "1",
            "cmd": "loadf",
            "params":  {
                "feed": "samples.data",
                "fstats": "samples.stats",
                "files": "samples.files.pcap",
                "schema": {"tshark": true }
            }
        }, "483d3420-38a8-4be0-8885-594e0643aa9f": {
            "version": "1",
            "cmd": "expand",
            "params":  {
                "feed": "samples.data.company",
                "input": "samples.data",
                "join": {
                    "datatype": "ituE.164",
                    "with": "directory.pbx",
                    "on": [{"field": "directory.pbx.TELEPHONE"}]
                },
                "add": {
                    "add": "${typefield}_COMPANY_ID",
                    "fields": [{"field": "data.pbx.COMPANY_ID"}],
                    "fmt": "{}"
                }
            }
        }, "d1a88ec3-f1bb-4f50-a46a-da94c73f20e7": {
            "version": "1",
            "cmd": "loads",
            "params":  {
                "feed": "samples.daily_stats",
                "input": "samples.stats",
                "fields": [],
                "partition": [{"field": {"f": "samples.stats.timestamp", "f": "day"}}]
            }
        }, "2a07b234-5bd4-44f1-9946-35a7721ddda9": {
            "version": "1",
            "cmd": "alert",
            "params":  {
                "input": "samples.files.new",
                "sumtype": "SUM",
                "email": "{EMAIL}",
                "period": 60
            }
        }, "6553d38a-a765-4d17-8a2d-16ca1de76d0c": {
            "version": "1",
            "cmd": "alert",
            "params":  {
                "feed": "samples.alert",
                "email": "{EMAIL}",
                "sumtype": "SUM",
                "period": 60
            }
        }
    }
}